<%- include('../components/header/table-header') %>

<div class="wrapper">
  
    <%- include('../components/sidebar/sidebar') %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Sinh viên</h1>
          </div>
         
        </div>
      </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card card-primary">
                        <div class="card-header">
                          <h3 class="card-title">Add</h3>
                        </div>
                       
                        <form action=<%= `` %> method="POST">
                            
                            <div class="card-body">
                                <% const text1 = luckyNumber.arr1.join(' '); %>
                                <% const text2 = luckyNumber.arr2.join(' '); %>
                                <% const text3 = luckyNumber.arr3.join(' '); %>
                                <% const text4 = luckyNumber.arr4.join(' '); %>
                                
                                <div class="form-group">
                                    <label for="arr1">Dãy 1</label>
                                    <input name="arr1" type="text" value="<%= text1 %>" class="form-control" id="arr1" autocomplete="none" required>
                                </div>
                                
                                <div class="form-group">
                                    <label for="arr2">Dãy 2</label>
                                    <input name="arr2" type="text" value="<%= text2 %>" class="form-control" id="arr2" autocomplete="none" required>
                                </div>
                        
                                <div class="form-group">
                                    <label for="arr3">Dãy 3</label>
                                    <input name="arr3" type="text" value="<%= text3 %>" class="form-control" id="arr3" autocomplete="none" required>
                                </div>
                        
                                <div class="form-group">
                                    <label for="arr4">Dãy 4</label>
                                    <input name="arr4" type="text" value="<%= text4 %>" class="form-control" id="arr4" autocomplete="none" required>
                                </div>

                            </div>
    
                            <div class="card-footer">
                                <button type="submit" class="btn btn-primary">Edit</button>
                            </div>
                        </form>
                        <!-- /.card-body -->
                      </div>
                    <!-- /.card -->
                </div>
            </div>
        </div><!--/. container-fluid -->
      <!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  <footer class="main-footer">
    <div class="float-right d-none d-sm-block">
      <b>Version</b> 3.2.0
    </div>
    <strong>Copyright &copy; 2014-2021 <a href="https://adminlte.io">AdminLTE.io</a>.</strong> All rights reserved.
  </footer>

  <!-- Control Sidebar -->
  <aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
  </aside>
  <!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->

<%- include('../components/footer/table-footer') %>